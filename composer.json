{
    "name": "alphazento/zento",
    "description": "A zento framework base on Laravel",
    "keywords": ["laravel", "zento"],
    "license": "MIT",
    "authors": [{
        "name": "Yongcheng.chen",
        "email": "yongcheng.chen@live.com"
    }],
    "require": {
        "php": ">=7.0",
        "illuminate/routing": "5.5.x|5.6.x|5.7.x|5.8.x",
        "illuminate/session": "5.5.x|5.6.x|5.7.x|5.8.x",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x"
    },
    "autoload": {
        "psr-4": {
            "Zento\\": "src/",
            "": ["mypackages/"]
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Zento\\Kernel\\Providers\\KernelProvider"
            ],
            "aliases": {
                "PackageManager": "Zento\\Kernel\\Facades\\PackageManager",
                "EventsManager": "Zento\\Kernel\\Facades\\EventsManager",
                "Debugger": "Zento\\Kernel\\Facades\\Debugger"
            }
        }
    },
    "require-dev": {
        "illuminate/framework": "5.5.x|5.6.x|5.7.x|5.8.x"
    }
}
